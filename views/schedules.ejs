        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
        <script src="https://cdn.anychart.com/releases/8.0.1/js/anychart-core.min.js"></script>
        <script src="https://cdn.anychart.com/releases/8.0.1/js/anychart-pie.min.js"></script>
        <script src="https://d3js.org/d3.v7.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/fullcalendar@5.10.1/main.js"></script>
        <base href="/">
        <link href="https://unpkg.com/tailwindcss@^1.0/dist/tailwind.min.css" rel="stylesheet">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/fullcalendar@5.10.1/main.css">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <script>

        document.addEventListener('DOMContentLoaded', function() {
          var calendarEl = document.getElementById('calendar');
          var calendar = new FullCalendar.Calendar(calendarEl, {
            initialView: 'timeGridWeek'
          });
          calendar.render();
        });

        function myFunction() {
          var x = document.getElementById("schedule");
          if (x.style.display === "none") {
            x.style.display = "block";
          } else {
            x.style.display = "none";
          }
        }

        function reload() {
          reload = location.reload();
        }

      </script>
</head>
<body background = "https://wallup.net/wp-content/uploads/2019/10/593967-clemson-tigers-college-football.jpg">
    <div style="text-align:center; padding-top:10px;">
    <button class="btn btn-primary" onclick="myFunction()">Toggle Schedule</button>
    <button class="btn btn-primary" onclick="window.location.reload()"><a href="/schedule"></a>Next Schedule</button>
    <div>
    <div class="place-items-center" id="schedule">

      <div class="w3/5 place-items-center" style = "margin:0 20%;" >
          <div id="calendar" style="height:800px; width:800px; margin-top: 20px; text-align:center;"></div>
      </div>

      <div id="container" class = "border-solid w2/5" style="height:280px; margin-bottom:150px">
      </div>

    </div >


</body>

